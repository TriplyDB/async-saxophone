{
  "name": "async-saxophone",
  "description": "Fast and lightweight asynchronous iterator XML parser in pure JavaScript",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "tape": "tape 'lib/**/*.test.js'",
    "test": "npm run --silent tape | faucet",
    "coverage": "nyc --reporter=html --reporter=text npm --silent run tape",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "lib/**/*.test.js"
    ]
  },
  "engines": {
    "node": "10 || 12 || 13 || 14"
  },
  "keywords": [
    "xml",
    "parser",
    "sax",
    "asynchronous",
    "async",
    "iterator",
    "fast"
  ],
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "benchmark": "^2.1.4",
    "common-tags": "^1.8.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.7.0",
    "faucet": "0.0.1",
    "lodash.uniq": "^4.5.0",
    "nyc": "^15.1.0",
    "tap-spec": "^5.0.0",
    "tape-async": "^2.3.0"
  },
  "dependencies": {}
}
